{
  "name": "@geoffcox/svelte-splitter",
  "version": "1.0.1",
  "description": "A splitter for Svelte that leverages CSS grid templates for simple and consistent resizing.",
  "author": "Geoff Cox",
  "license": "MIT",
  "bugs": {
    "url": "https://github.com/GeoffCox/svelte-splitter/issues"
  },
  "homepage": "https://github.com/GeoffCox/svelte-splitter/blob/main/package/README.md",
  "keywords": [
    "split",
    "splitter",
    "resize",
    "resizer",
    "pane",
    "svelte",
    "typescript"
  ],
  "module": "dist/index.mjs",
  "main": "dist/index.js",
  "svelte": "src/index.ts",
  "types": "src/index.d.ts",
  "files": [
    "src",
    "dist"
  ],
  "scripts": {
    "emitTypes": "tsc --emitDeclarationOnly",
    "build": "rollup -c",
    "dev": "rollup -c -w",
    "prepublishOnly": "npm run build",
    "validate": "svelte-check"
  },
  "devDependencies": {
    "@rollup/plugin-node-resolve": "^11.0.0",
    "@rollup/plugin-typescript": "^8.0.0",
    "@tsconfig/svelte": "^1.0.0",
    "rollup": "^2.3.4",
    "rollup-plugin-commonjs": "^10.1.0",
    "rollup-plugin-css-only": "^3.1.0",
    "rollup-plugin-svelte": "^7.0.0",
    "rollup-plugin-terser": "^7.0.0",
    "svelte": "^3.0.0",
    "svelte-check": "^1.0.0",
    "svelte-preprocess": "^4.0.0",
    "tslib": "^2.0.0",
    "typescript": "^4.0.0"
  },
  "dependencies": {}
}
